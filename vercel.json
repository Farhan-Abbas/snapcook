{
    "rewrites": [
        {
            "source": "/api/imageGenerator",
            "destination": "/api/imageGenerator"
        },
        {
            "source": "/(.*).(js|css|png|jpg|jpeg|gif|svg|ico)",
            "destination": "/src/$1.$2"
        },
        {
            "source": "/(.*)",
            "destination": "/src/index.html"
        }
    ]
}